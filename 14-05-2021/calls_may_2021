    ActiveWorkbook.Worksheets("output").AutoFilter.Sort.SortFields.Add Key:=Range _
        ("D1:D76"), SortOn:=xlSortOnValues, Order:=xlDescending, DataOption:= _
        xlSortNormal
    With ActiveWorkbook.Worksheets("output").AutoFilter.Sort
        .Header = xlYes
